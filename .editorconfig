# ----------------------------------------------------------------------------------------------------------------------

### .NET rules

[*.{cs, vb}]

# 'this.' and 'Me.' qualifiers
dotnet_diagnostic.IDE0003.severity = warning
dotnet_diagnostic.IDE0009.severity = warning
dotnet_style_qualification_for_field = true:warning
