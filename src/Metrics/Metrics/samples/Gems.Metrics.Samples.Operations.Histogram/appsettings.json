{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "PrometheusMetrics": {
    "Configuration": {
      "HistogramConfiguration": {
        "persons_age_histogram": {
          "Buckets": [ 18, 25, 30, 40, 50, 60 ] // Шкала гистограммы по возрасту персон
        }
      }
    }
  }
}
