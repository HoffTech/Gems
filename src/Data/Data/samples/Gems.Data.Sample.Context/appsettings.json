{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",

  "UnitOfWorkProviderOptions": {
    "UseContext": true
  },
  
  "PostgresqlUnitOfWorks": [
    {
      "Key": "default",
      "Options": {
        "ConnectionString": "${ConnectionStrings.DefaultConnection}"
      }
    }
  ]
}
